{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListBuckets",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": [
        "arn:aws:s3:::cloudlaunch-site-bucket347",
        "arn:aws:s3:::cloudlaunch-private-bucket347",
        "arn:aws:s3:::cloudlaunch-visible-only-bucket347"
      ]
    },
    {
      "Sid": "SiteBucketRead",
      "Effect": "Allow",
      "Action": "s3:GetObject",
      "Resource": "arn:aws:s3:::cloudlaunch-site-bucket347/*"
    },
    {
      "Sid": "PrivateBucketReadWrite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject"
      ],
      "Resource": "arn:aws:s3:::cloudlaunch-private-bucket347/*"
    }
  ]
}
